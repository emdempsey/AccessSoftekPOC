            },
            "caching": "ReadWrite",
            "createOption": "FromImage"
          }
        },
        "networkProfile": {
          "networkInterfaces": [
            {
              "id": "[concat(resourceId('Microsoft.Network/networkInterfaces',variables('OrphAppnicName')), '2')]"
            }
          ]
        }
      }
    },
    //ORPH 3 DSC
    {
      "apiVersion": "2015-05-01-preview",
